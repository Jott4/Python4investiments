nitas; and Emory Healthcare to develop and test 5G Ultra Wideband-enabled use cases. The company was formerly known as Bell Atlantic Corporation and changed its name to Verizon Communications Inc. in June 2000. Verizon Communications Inc. was founded in 1983 and is headquartered in New York, New York.","fullTimeEmployees":135500,"companyOfficers":[{"maxAge":1,"name":"Mr. Hans E. Vestberg","age":54,"title":"Chairman & CEO","yearBorn":1965,"fiscalYear":2019,"totalPay":{"raw":6095279,"fmt":"6.1M","longFmt":"6,095,279"},"exercisedValue":{"raw":0,"fmt":null,"longFmt":"0"},"unexercisedValue":{"raw":0,"fmt":null,"longFmt":"0"}},{"maxAge":1,"name":"Mr. Matthew D. Ellis","age":48,"title":"Exec. VP & CFO","yearBorn":1971,"fiscalYear":2019,"totalPay":{"raw":2748885,"fmt":"2.75M","longFmt":"2,748,885"},"exercisedValue":{"raw":0,"fmt":null,"longFmt":"0"},"unexercisedValue":{"raw":0,"fmt":null,"longFmt":"0"}},{"maxAge":1,"name":"Ms. Tami A. Erwin","age":54,"title":"Exec. VP & Group CEO of Verizon Bus. Group","yearBorn":1965,"fiscalYear":2019,"totalPay":{"raw":2483297,"fmt":"2.48M","longFmt":"2,483,297"},"exercisedValue":{"raw":0,"fmt":null,"longFmt":"0"},"unexercisedValue":{"raw":0,"fmt":null,"longFmt":"0"}},{"maxAge":1,"name":"Mr. Ronan J. Dunne","age":56,"title":"Exec. VP & Group CEO of Verizon Consumer Group","yearBorn":1963,"fiscalYear":2019,"totalPay":{"raw":2953376,"fmt":"2.95M","longFmt":"2,953,376"},"exercisedValue":{"raw":0,"fmt":null,"longFmt":"0"},"unexercisedValue":{"raw":0,"fmt":null,"longFmt":"0"}},{"maxAge":1,"name":"Mr. K. Guru Gowrappan","age":38,"title":"Exec. VP & Group CEO of Verizon Media Group","yearBorn":1981,"fiscalYear":2019,"totalPay":{"raw":2785858,"fmt":"2.79M","longFmt":"2,785,858"},"exercisedValue":{"raw":0,"fmt":null,"longFmt":"0"},"unexercisedValue":{"raw":0,"fmt":null,"longFmt":"0"}}],"auditRisk":1,"boardRisk":6,"compensationRisk":2,"shareHolderRightsRisk":5,"overallRisk":3,"governanceEpochDate":1575676800,"compensationAsOfEpochDate":1577750400,"m